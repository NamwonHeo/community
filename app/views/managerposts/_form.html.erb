
<%= simple_form_for(@managerpost) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :published %>
  </div>

  <div class="form-actions">
    <button type="button" class="btn btn-outline-primary"><%= f.button :submit %></button>
  </div>
<% end %>
